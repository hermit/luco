<Window x:Class="Luco.ServerAddDialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="サーバの追加" Height="300" Width="300">
  <Grid>
    <Label Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" HorizontalAlignment="Left">サーバの種類</Label>
    <ComboBox Margin="106,12,52,0" Name="serverTypeComboBox" VerticalAlignment="Top">
      <ComboBoxItem>RSS</ComboBoxItem>
      <ComboBoxItem>IRC</ComboBoxItem>
      <ComboBoxItem>Twitter</ComboBoxItem>
    </ComboBox>
    <Button IsDefault="True" Height="27.5" Margin="70,0,113,12" VerticalAlignment="Bottom" Click="okButton_Click">OK</Button>
    <Button IsCancel="True" Height="27.5" Margin="0,0,12,12" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="95">キャンセル</Button>
  </Grid>
</Window>
